<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <link href="~/Content/themes/base/minified/jquery.ui.dialog.min.css" rel="stylesheet" />
    @RenderSection("StyleSection", false)
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>MVC4</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Auto 1", "Sample", "Autocomplete")</li>
                    <li>@Html.ActionLink("Auto 2", "Index", "Autocomplete")</li>
                    <li>@Html.ActionLink("Remote", "Index", "Remote")</li>
                    <li>@Html.ActionLink("Validation", "Index", "Person")</li>
                    <li>@Html.ActionLink("Dialog", "Example", "Person")</li>
                    <li>@Html.ActionLink("Tabs", "Index", "Tabs")</li>
                    <li>@Html.ActionLink("Notification", "Index", "Notification")</li>
                    <li>@Html.ActionLink("Grid", "Index", "Grid")</li>
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
        </div>
    </div>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.0.min.js")" type="text/javascript"></script>
    @RenderSection("ScriptSection", false)
</body>
</html>
